
<h1> <%= current_user.display_name %>'s Task Lists </h1>

<table class="table table-condensed table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @unarchived.reverse.each do |list| %>
      <tr>
        <td><%= link_to list.title, task_list_path(list.id) %></td>
        <td><%= link_to 'Edit', edit_task_list_path(list.id)%></td>
      </tr>
    <% end %>
  </tbody>
</table>

  <%= render 'task_lists/archived' %>



<%= link_to 'Create A New Task List', new_task_list_path, class: 'btn btn-default btn-s' %><br><br>



