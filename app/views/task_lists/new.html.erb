
<div class="col-xs-6">
    <div class="form">
      <h1>Create A Task List</h1>

        <%= form_for(:task_list, url: task_lists_path, method: :post, role:"form") do |f| %>

          <%= f.label :title %>
          <%= f.text_field :title, class: "form-control"%><br>

        <div>
          <%= f.submit "Create A Task List", class: "btn btn-primary" %>
        </div>
        <% end %>
    </div>  
</div>